# Copy to .env and fill values as needed. Do not commit .env.

# Runtime
NODE_ENV=development
SESSION_SECRET=dev-change-me-session
CORS_ORIGIN=http://localhost:3000

# Persistence (optional; example for SQLite file paths)
# PERSIST_DB_URL=

# LiveKit (optional for voice features)
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=
LIVEKIT_ROOM=agent-console

# App (Next.js) server -> Agent HTTP URL
AGENT_HTTP_URL=http://localhost:4001

# Rate limiting (app)
RATE_LIMIT_WINDOW_MIN=5
RATE_LIMIT_MAX=60

# Agent service
PORT=4001
# AGENT_PORT=

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4.1-mini
OPENAI_API_BASE_URL=https://api.openai.com/v1

# Speech
ASR_MODEL=whisper-1
TTS_MODEL=gpt-4o-mini-tts
TTS_VOICE=alloy
TTS_FORMAT=mp3

# Observability (optional)
OTEL_SERVICE_NAME=freya-agent
# OTEL_EXPORTER_OTLP_ENDPOINT=
# OTEL_EXPORTER_OTLP_HEADERS=
LOG_LEVEL=info

# Dev fallback response (agent)
FALLBACK_RESPONSE=This is a simulated response from the Freya agent service.

