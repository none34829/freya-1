{
  "name": "agent",
  "version": "0.1.0",
  "description": "LiveKit agent service for Freya console",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "dev": "tsx watch src/index.ts",
    "dev:agent": "tsx src/livekit-agent.ts dev",
    "download-files": "tsx src/livekit-agent.ts download-files",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "pino-pretty": "^13.1.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.18.0",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/websocket": "^11.2.0",
    "@livekit/agents": "^1.0.9",
    "@livekit/agents-plugin-livekit": "^1.0.9",
    "@livekit/agents-plugin-silero": "^1.0.9",
    "@livekit/noise-cancellation-node": "^0.1.9",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "livekit-server-sdk": "^2.14.0",
    "music-metadata": "^10.5.1",
    "openai": "^6.2.0",
    "pino": "^10.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  }
}
